<?xml version="1.0" encoding="UTF-8"?>
<dataset>
	<table name="supplier">
		<column>supplier_id</column>
		<column>name</column>
		<column>nit</column>
		<column>telephone</column>
		<column>address</column>
		<column>email</column>
		<column>contact</column>
	</table>
	
	<table name="product_supplier">
		<column>product_id</column>
		<column>supplier_id</column>
		<column>sku</column>
	</table>
	
	<table name="receipt">
		<column>receipt_id</column>
		<column>user_account_username</column>
		<column>supplier_id</column>
		<column>date</column>
		<column>shipment_number</column>
		<column>total</column>
		<column>status</column>
	</table>
	
	<table name="purchase_return">
		<column>purchase_return_id</column>
		<column>user_account_username</column>
		<column>supplier_id</column>
		<column>date</column>
		<column>reason</column>
		<column>contact</column>
		<column>total</column>
		<column>status</column>
	</table>
</dataset>